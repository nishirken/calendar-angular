{
  "name": "calendar-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "typecheck": "tsc --noEmit -p tsconfig.spec.json && tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint ./src --ext .ts,.tsx --quiet",
    "lint:fix": "npm run lint -- --fix",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "test:ci": "TZ=UTC jest --runInBand"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "calendar-header": "github:nishirken/calendar-header#10570cb",
    "calendar-test-ids": "github:nishirken/calendar-test-ids#86715e3",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "rxjs": "7.5.0",
    "tslib": "2.3.0",
    "wired-elements": "github:nishirken/wired-elements#efc1835",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular/cli": "~14.2.7",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jest": "29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.3.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.3",
    "prettier-eslint": "^15.0.1",
    "typescript": ">= 4.7"
  }
}
